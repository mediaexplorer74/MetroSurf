<local:BasePage
    x:Class="VPN.View.LoginKeepSolidIDPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:VPN.View">
    <Grid x:Name="LayoutRoot">
        <StackPanel>
            <ProgressBar x:Name="ProgressBar" Visibility="Collapsed" />
            <TextBox x:Name="LoginTextBox" KeyUp="LoginTextBoxOnKeyUp" />
            <PasswordBox x:Name="PasswordBox" KeyUp="PasswordBoxOnKeyUp" />
        </StackPanel>
    </Grid>
</local:BasePage>
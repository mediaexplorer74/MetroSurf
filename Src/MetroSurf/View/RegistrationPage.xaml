<local:BasePage
    x:Class="VPN.View.RegistrationPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:VPN.View">
    <Grid x:Name="LayoutRoot" Loaded="BasePage_Loaded">
        <StackPanel>
            <ProgressBar x:Name="ProgressBar" Visibility="Collapsed"/>
            <TextBox x:Name="LoginTextbox" KeyUp="LoginTextBoxOnKeyUp"/>
            <PasswordBox x:Name="PasswordBox" KeyUp="PasswordBoxOnKeyUp1"/>
            <PasswordBox x:Name="ConfirmPasswordBox" KeyUp="PasswordBoxOnKeyUp"/>
        </StackPanel>
    </Grid>
</local:BasePage>